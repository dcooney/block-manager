{
	"name": "block-manager",
	"version": "3.2.0",
	"description": "Take control of your WordPress Blocks & Patterns.",
	"author": "connekthq.com <support@connekthq.com>",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"WordPress",
		"plugin",
		"blocks"
	],
	"scripts": {
		"watch": "npm run dev",
		"dev": "wp-scripts start",
		"start": "webpack -w --mode development --config=webpack/dev.config.js",
		"build": "npm run build:pot && wp-scripts build",
		"build:pot": "composer run pot",
		"lint:php": "composer run lint",
		"postinstall": "composer install"
	},
	"dependencies": {
		"@wordpress/block-editor": "^15.9.0",
		"@wordpress/block-library": "^9.36.0",
		"@wordpress/blocks": "^15.9.0",
		"@wordpress/dom-ready": "^4.36.0",
		"@wordpress/element": "^6.36.0",
		"@wordpress/hooks": "^4.36.0",
		"@wordpress/i18n": "^6.9.0"
	},
	"devDependencies": {
		"@wordpress/scripts": "^31.1.0",
		"axios": "^1.5.1",
		"classnames": "^2.5.1",
		"npm-run-all": "^4.1.5",
		"postcss-import": "^16.1.1",
		"postcss-preset-env": "^10.4.0",
		"postcss-scss": "^4.0.9",
		"prettier": "^3.7.4",
		"stylelint-config-prettier": "^9.0.5",
		"webpack-cli": "^6.0.1",
		"webpack-merge": "^6.0.1"
	}
}
